                    <input type = 'text' class="form-control" rows="3" name="resumeName" id="resumeName" placeholder="Name of Resume" required="required">
                    <input type = 'text' class="form-control" rows="3" name="Message" id="Message" placeholder="Message" required="required">

              </div>
            </div>
            </div>
            <div class="row">
              <div class="col-md-12">

                <input type="submit" id="postIt" class="btn main-btn" value = "postComment" name=""/>
              </div>
            </div></form>
